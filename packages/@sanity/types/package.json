{
  "name": "@sanity/types",
  "version": "2.0.1",
  "description": "Type definitions for common Sanity data structures",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:cjs && npm run build:dts && npm run build:assets",
    "build:assets": "cpx 'src/**/*.{css,json,svg}' lib",
    "build:cjs": "babel src --extensions '.ts,.tsx,.js' --out-dir lib",
    "build:dts": "tsc -p tsconfig.dist.json --emitDeclarationOnly",
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "type-check": "tsc --noEmit -p tsconfig.dist.json"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "typescript"
  ],
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "cpx": "^1.5.0",
    "react": "^16.14.0",
    "rimraf": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/"
}
