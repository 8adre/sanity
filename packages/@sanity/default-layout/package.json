{
  "name": "@sanity/default-layout",
  "version": "2.0.4",
  "description": "The default layout components for Sanity",
  "main": "lib/root.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:dts && npm run build:assets",
    "build:assets": "cpx 'src/**/*.{css,json,svg}' lib",
    "build:cjs": "babel src --extensions '.ts,.tsx,.js' --out-dir lib",
    "build:dts": "tsc -p tsconfig.dist.json --emitDeclarationOnly",
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "posttest": "npm run type-check",
    "type-check": "tsc --noEmit -p tsconfig.dist.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "default-layout",
    "layout"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/",
  "dependencies": {
    "@sanity/components": "2.0.4",
    "@sanity/generate-help-url": "2.0.1",
    "@sanity/ui": "^1.0.0-alpha.26",
    "@sanity/util": "2.0.1",
    "lodash": "^4.17.15",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@sanity/base": "2.0.4",
    "@types/styled-components": "^5.1.4",
    "cpx": "^1.5.0",
    "react": "^16.14.0",
    "rimraf": "^2.7.1",
    "styled-components": "^5.2.0"
  },
  "peerDependencies": {
    "@sanity/base": "^2.0",
    "react": "^16.9",
    "styled-components": "^5.2"
  }
}
